<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/exploreFragment">
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.romp.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment_to_tournament"
            app:destination="@id/tournament" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.romp.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/myteam"
        android:name="com.example.romp.Myteam"
        android:label="fragment_myteam"
        tools:layout="@layout/fragment_myteam" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.romp.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
    <activity
        android:id="@+id/mainActivity2"
        android:name="com.example.romp.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/tournament"
        android:name="com.example.romp.tournament"
        android:label="fragment_tournament"
        tools:layout="@layout/fragment_tournament" />
</navigation>