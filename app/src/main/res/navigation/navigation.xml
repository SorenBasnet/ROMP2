<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/exploreFragment">
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.romp.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/action_exploreFragment_to_comingsoon"
            app:destination="@id/comingsoon"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.romp.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_formFragment"
            app:destination="@id/formFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/action_homeFragment_to_formFragment2"
            app:destination="@id/formFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/action_homeFragment_to_fragment_tournamentdetails"
            app:destination="@id/fragment_tournamentdetails"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>
    <fragment
        android:id="@+id/myteam"
        android:name="com.example.romp.Myteam"
        android:label="fragment_myteam"
        tools:layout="@layout/fragment_myteam" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.romp.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/formFragment"
        android:name="com.example.romp.FormFragment"
        android:label="form_fragment"
        tools:layout="@layout/fragment_form">
        <action
            android:id="@+id/action_formFragment_to_tournamentSuccess"
            app:destination="@id/tournamentSuccess"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>
    <activity
        android:id="@+id/mainActivity2"
        android:name="com.example.romp.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/comingsoon"
        android:name="com.example.romp.comingsoon"
        android:label="fragment_comingsoon"
        tools:layout="@layout/fragment_comingsoon" />
    <fragment
        android:id="@+id/fragment_tournamentdetails"
        android:name="com.example.romp.fragment_tournamentdetails"
        android:label="fragment_tournamentdetails"
        tools:layout="@layout/fragment_tournamentdetails" />
    <fragment
        android:id="@+id/tournamentSuccess"
        android:name="com.example.romp.TournamentSuccess"
        android:label="TournamentSuccess" />
</navigation>